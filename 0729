            success: function (data, status, xhr) {
                var dataresults = data.d;
                console.log(dataresults);
buid=dataresults.BUIDId;
document.getElementById('ptitle').value=dataresults.Title;
document.getElementById('facility').value=dataresults.Facility_x002f_Terminal;
document.getElementById('pnumber').value=dataresults.Project_x0020_Number;
document.getElementById('pmanager').value=dataresults.Project_x0020_Manager;
