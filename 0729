  <!-- Global Variables -->
		
		    <!-- Global Variables -->
		  
		  function getQueryString() {
          var key = false, res = {}, itm = null;
          // get the query string without the ?
          var qs = location.search.substring(1);
          // check for the key as an argument
          if (arguments.length > 0 && arguments[0].length > 1)
            key = arguments[0];
          // make a regex pattern to grab key/value
          var pattern = /([^&=]+)=([^&]*)/g;
          // loop the items in the query string, either
          // find a match to the argument, or build an object
          // with key/value pairs
            while (itm = pattern.exec(qs)) {
            if (key !== false && decodeURIComponent(itm[1]) === key)
            return decodeURIComponent(itm[2]);
            else if (key === false)
              res[decodeURIComponent(itm[1])] = decodeURIComponent(itm[2]);
          }

          return key === false ? res : null;
    }
    queryParam = getQueryString();
    var buid = queryParam.BUID;
   
    queryParams = getQueryString();
    var mode = queryParams.Mode;

 
<!-- Get ValueByID Function -->
		  function getvalue(id) {
                 var ele = $("#"+ id+"");
                var type = ele[0].nodeName.toLowerCase();
                switch (type) {
                    case "input":
                        return ele.val();
                        break;
                    case "select":
                        return $(":selected", ele).val()
                        break;
                }
            }

 <!-- Get/Fetch FormData Function -->
 
function getformdata(metadata){
var data = {};
data.__metadata = metadata;
//data.BU_x0020_RequestId =parseInt(buid);   
data.BUIDId =parseInt(buid);   
data.Title= getvalue("ptitle");
data.Facility_x002f_Terminal= getvalue("facility");
data.Project_x0020_Number=getvalue("pnumber");
data.Project_x0020_Manager=getvalue("pmanager");
data.orcn=getvalue("mdate");
data.Signature=getvalue("signature");

//data.r8cu=getvalue("t1q1r");
data.efbz=getvalue("t1q1c");
data.tmxn=getvalue("t1q1d");
//data.ot6s=getvalue("t1q2r");
data.wo8a=getvalue("t1q2c");
data.Task1_Q2D=getvalue("t1q2d");
//data.Task1_x0020_Q3R=getvalue("t1q3r");
data.q5hi=getvalue("t1q3c");
data.wfgm=getvalue("t1q3d");
//data.Task_x0020_1_x0020_Q4R=getvalue("t1q4r");
data.Task_x0020_1_x0020_Q4C=getvalue("t1q4c");
data.Task_x0020_1_x0020_Q4D=getvalue("t1q4d");

//data.Task_x0020_2_x0020_Q1R=getvalue("t2q1r");
data.Task_x0020_2_x0020_Q1C=getvalue("t2q1c");
data.Task_x0020_2_x0020_Q1D=getvalue("t2q1d");
//data.Task_x0020_2_x0020_Q2R=getvalue("t2q2r");
data.Task_x0020_2_x0020_Q2C=getvalue("t2q2c");
data.Task_x0020_2_x0020_Q2D=getvalue("t2q2d");
//data.Task_x0020_2_x0020_Q3R=getvalue("t2q3r");
data.Task_x0020_2_x0020_Q3C=getvalue("t2q3c");
data.Task_x0020_2_x0020_Q3D=getvalue("t2q3d");
//data.Task_x0020_2_x0020_Q4R=getvalue("t2q4r");
data.Task_x0020_2_x0020_Q4C=getvalue("t2q4c");
data.Task_x0020_2_x0020_Q4D=getvalue("t2q4d");
//data.Task_x0020_2_x0020_Q5R=getvalue("t2q5r");
data.Task_x0020_2_x0020_Q5C=getvalue("t2q5c");
data.Task_x0020_2_x0020_Q5D=getvalue("t2q5d");

//data.Task_x0020_3_x0020_Q1R=getvalue("t3q1r");
data.Task_x0020_3_x0020_Q1C=getvalue("t3q1c");
data.Task_x0020_3_x0020_Q1D=getvalue("t3q1d");
//data.Task_x0020_3_x0020_Q2R=getvalue("t3q2r");
data.Task_x0020_3_x0020_Q2C=getvalue("t3q2c");
data.Task_x0020_3_x0020_Q2D=getvalue("t3q2d");
//data.Task_x0020_3_x0020_Q3R=getvalue("t3q3r");
data.Task_x0020_3_x0020_Q3C=getvalue("t3q3c");
data.Task_x0020_3_x0020_Q3D=getvalue("t3q3d");

$(".table [type=checkbox]").each(function(i,e){
data[$(e).attr("id")]= $(e)[0].checked?"on":"off"
              })
console.log(data,"checkbox data")

	return JSON.stringify(data);
}




